# Production environment for Railway deployment

# Next.js
NODE_ENV=production

# Database Adapter - Use Supabase in production
USE_SUPABASE=true

# These should be set in Railway environment variables
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
# SUPABASE_SERVICE_ROLE_KEY=

# Feature Flags
REQUIRE_AUTH=false
IS_MOBILE=false